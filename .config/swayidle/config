timeout 30  'pidof -q swaylock && swaymsg -q "output * power off"' resume 'pidof -q swaylock && swaymsg -q "output * power on"'
timeout 300 'pidof -q swaylock || swaymsg -q "output * power off"' resume 'pidof -q swaylock || swaymsg -q "output * power on"'
timeout 305 'swaylock'
before-sleep 'playerctl pause'
before-sleep 'swaylock'
lock 'swaylock'
unlock 'killall -USR1 swaylock'
